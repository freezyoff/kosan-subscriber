<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <stroke
                android:width="1dp"
                android:color="#60222222" />
            <solid android:color="#00FD23" />
            <padding
                android:bottom="8dp"
                android:left="8dp"
                android:right="8dp"
                android:top="8dp" />
        </shape>
    </item>
    <item>
        <shape android:shape="oval">
            <stroke
                android:width="1dp"
                android:color="#8000FD23" />
            <solid android:color="#222222" />
            <padding
                android:bottom="32dp"
                android:left="32dp"
                android:right="32dp"
                android:top="32dp" />
        </shape>
    </item>
    <item
        android:width="32dp"
        android:height="32dp"
        android:drawable="@drawable/ic_lock_closed"
        android:gravity="center" />

</layer-list>